{
  "name": "Event",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "description": "Event name"
    },
    "location": {
      "type": "string",
      "description": "Event location in Dubai"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "start_time": {
      "type": "string"
    },
    "end_time": {
      "type": "string"
    },
    "category": {
      "type": "string",
      "enum": [
        "festival",
        "conference",
        "sports",
        "concert",
        "exhibition",
        "shopping"
      ],
      "description": "Type of event"
    },
    "expected_crowd": {
      "type": "string",
      "enum": [
        "low",
        "medium",
        "high",
        "very_high"
      ],
      "default": "medium"
    },
    "description": {
      "type": "string"
    },
    "affected_areas": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Areas that might be crowded"
    }
  },
  "required": [
    "title",
    "location",
    "date"
  ]
}